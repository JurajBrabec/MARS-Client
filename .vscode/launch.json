{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via NPM",
      "runtimeExecutable": "P:/PortableApps/NodeJSPortable/App/npm",
      "runtimeArgs": ["run-script", "debug"],
      "port": 9229,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "runtimeExecutable": "P:/PortableApps/NodeJSPortable/App/node",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\src\\index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Script",
      "runtimeExecutable": "P:/PortableApps/NodeJSPortable/App/node",
      "skipFiles": ["<node_internals>/**"],
      "program": "${file}",
      "env": { "DEBUG": "*" }
    }
  ]
}
